      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.astrolabe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
