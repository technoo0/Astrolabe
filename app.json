{
  "expo": {
    "name": "Astrolabe",
    "slug": "astrolabe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon.png",
    "splash": {
      "image": "./assets/TeamLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E24"
    },
    "updates": {
